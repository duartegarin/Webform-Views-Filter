<?php
/**
 * @file
 * Primarily file for this Drupal module.
 *
 * We define the api version and the path to the views plugins created.
 */

/**
 * Implements hook_views_api().
 */
function webform_views_filter_views_api() {
  return array(
    'api' => 3,
    'path' => drupal_get_path('module', 'webform_views_filter') . '/views'
  );
}